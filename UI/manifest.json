{
  "manifest_version": 3,
  "name": "UT Wizz",
  "version": "1.0",
  "description": "FSD scanner to create Unit tests",
  "permissions": ["activeTab", "scripting", "notifications", "http://127.0.0.1:8000/"],
  "host_permissions": ["http://127.0.0.1:8000/"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  }
}
